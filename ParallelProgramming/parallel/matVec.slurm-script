#!/bin/csh
#SBATCH -p par6.q
#SBATCH -N 2
#SBATCH --ntasks-per-node=16
#SBATCH -t 1:00:00
#SBATCH --exclusive

module purge
module load slurm
module load intelmpi/intel/5.0.3

mpirun ./matMat 10 10
